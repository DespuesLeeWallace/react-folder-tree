{
  "name": "react-folder-tree",
  "version": "4.0.0",
  "description": "folder tree library with React + Hooks",
  "main": "src/index.js",
  "repository": "git@github.com:shunjizhan/React-Folder-Tree.git",
  "author": "Shunji Zhan <shunhahaha@live.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "start": "NODE_ENV=development node scripts/dev-server.js",
    "lint": "eslint src/ --ext .js,.jsx",
    "lint-fix": "eslint src/ --ext .js,.jsx --fix",
    "test": "jest --collect-coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
